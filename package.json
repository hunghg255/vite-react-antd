{
  "name": "template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run lint && tsc && vite build",
    "buildDev": "npm run lint && tsc && vite build --mode development",
    "buildProd": "npm run lint && tsc && vite build --mode production",
    "preview": "vite preview --host",
    "start": "npm run build && vite preview",
    "lint": "eslint --ext .ts,.tsx src --color",
    "format": "prettier --write \"./src/**/*.{ts,tsx,json}\"",
    "gen-icon": "csvg-to-font",
    "analyze": "npm run lint && tsc && vite build --mode=analyze && source-map-explorer 'dist/assets/*.js'",
    "verify-commit": "verify-commit-msg",
    "postinstall": "git-scm-hooks",
    "release": "bumpp -r"
  },
  "dependencies": {
    "ahooks": "^3.7.8",
    "antd": "^5.10.2",
    "brainless-token-manager": "^1.3.3",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.3.0",
    "jotai": "^1.13.1",
    "jotai-devtools": "^0.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.17.0",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@hunghg255/eslint-config-react": "^0.0.16",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/react-helmet": "^6.1.8",
    "@vitejs/plugin-react": "^4.1.0",
    "antd-form-devtools": "^1.0.2",
    "bumpp": "^9.2.0",
    "csvg-to-font": "^1.0.15",
    "eslint": "^8.52.0",
    "eslint-config-react-app": "^7.0.1",
    "git-scm-hooks": "^0.0.6",
    "prettier": "^2.8.8",
    "sass": "^1.69.5",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.9.5",
    "verify-commit-msg": "^0.0.11",
    "vite": "^4.5.0",
    "vite-host-qrcode": "^0.0.4",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-resize-image": "^0.0.4",
    "vite-plugin-webfont-dl": "^3.7.6"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
