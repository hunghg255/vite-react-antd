{
  "name": "template",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.15.9",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "buildDev": "vite build --mode development",
    "buildProd": "vite build --mode production",
    "preview": "vite preview --host",
    "start": "npm run build && vite preview",
    "lint": "eslint src --quiet",
    "fix": "eslint --fix src",
    "analyze": "vite build --mode=analyze && source-map-explorer 'dist/assets/*.js'",
    "verify-commit": "verify-commit-msg",
    "type-check": "tsc --noEmit",
    "prepare": "git-scm-hooks",
    "release": "bumpp -r",
    "gen-icon": "csvgtocss",
    "gen-color": "esno ./scripts/genColorCss.ts"
  },
  "dependencies": {
    "ahooks": "^3.8.4",
    "antd": "^5.22.3",
    "brainless-token-manager": "^1.3.3",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.7.1",
    "jotai": "^1.13.1",
    "jotai-devtools": "^0.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^14.1.3",
    "react-router-dom": "^6.28.0",
    "sonner": "^1.7.0",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@hunghg255/eslint-config": "^0.0.14",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.17.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "antd-form-devtools": "^1.0.2",
    "bumpp": "^9.8.1",
    "csvgtocss": "^0.1.2",
    "eslint": "^9.16.0",
    "esno": "^4.8.0",
    "git-scm-hooks": "^0.2.0",
    "react-generate-pages": "^0.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.82.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.7.2",
    "verify-commit-msg": "^0.1.0",
    "vite": "^5.4.11",
    "vite-console-debug": "^1.1.10",
    "vite-host-qrcode": "^0.0.4",
    "vite-minify-css-module": "^1.0.3",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-resize-image": "^0.0.4",
    "vite-plugin-webfont-dl": "^3.10.2"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
