{
  "name": "template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run lint && tsc && vite build",
    "buildDev": "npm run lint && tsc && vite build --mode development",
    "buildProd": "npm run lint && tsc && vite build --mode production",
    "preview": "vite preview --host",
    "start": "npm run build && vite preview",
    "lint": "eslint --ext .ts,.tsx src --color --cache --quiet",
    "format": "prettier --write \"./src/**/*.{ts,tsx,json}\"",
    "analyze": "npm run lint && tsc && vite build --mode=analyze && source-map-explorer 'dist/assets/*.js'",
    "verify-commit": "verify-commit-msg",
    "prepare": "git-scm-hooks",
    "release": "bumpp -r",
    "gen-icon": "csvgtocss",
    "gen-color": "esno ./scripts/genColorCss.ts"
  },
  "dependencies": {
    "ahooks": "^3.7.11",
    "antd": "^5.17.2",
    "brainless-token-manager": "^1.3.3",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.11",
    "i18next": "^23.11.4",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.2",
    "jotai": "^1.13.1",
    "jotai-devtools": "^0.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.23.1",
    "sonner": "^1.4.41",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@hunghg255/eslint-config-react": "^0.0.26",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.2.1",
    "antd-form-devtools": "^1.0.2",
    "bumpp": "^9.4.1",
    "csvgtocss": "^0.0.8",
    "eslint": "^8.57.0",
    "git-scm-hooks": "^0.0.11",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.2",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.9.5",
    "verify-commit-msg": "^0.0.14",
    "vite": "^5.2.11",
    "vite-console-debug": "^1.1.10",
    "vite-host-qrcode": "^0.0.4",
    "vite-minify-css-module": "^1.0.3",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-resize-image": "^0.0.4",
    "vite-plugin-webfont-dl": "^3.9.4"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
